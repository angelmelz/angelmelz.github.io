<!DOCTYPE html>
<html>
<script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.9.4/Chart.js"></script>
<body>
<!-- I made a pie chart for the percentage of time spent performing basic tasks -->
<canvas id="myChart" style="width:100%;max-width:600px"></canvas>

<script>
var xValues = ["Sleeping", "Trying to sleep", "Working", "Screen Time", "Eating"];
var yValues = [33, 8, 16, 14, 5];
var barColors = [
  "#ff33cc",
  "#330099",
  "#0099FF",
  "#CC99FF",
  "#99FFCC"
];

new Chart("myChart", {
  type: "pie",
  data: {
    labels: xValues,
    datasets: [{
      backgroundColor: barColors,
      data: yValues
    }]
  },
  options: {
    title: {
      display: true,
      text: "Average Time Spent Doing Tasks in Lifetime"
    }
  }
});
</script>

